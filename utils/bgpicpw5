convert -density 300 -depth 8 [0-9][0-9]0.png -append -stroke black -strokewidth 4 -draw "line 2,0 2,1236" -draw "line 824,124 824,864" -draw "line 1645,0 1645,1236" ../zman/basepw5.png

rm ./[0-9][0-9]0.png
