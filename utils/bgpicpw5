convert -density 300 -depth 8 -size 1648x124 canvas:lightgrey -fill black -stroke black -strokewidth 10.8 -draw "line 0,124 1648,124" 010.png
convert -density 300 -depth 8 -size 1648x124 canvas:lightgrey -fill black -stroke black -strokewidth 10.8 -draw "line 0,124 1648,124" 020.png
convert -density 300 -depth 8 -size 1648x124 canvas:lightgrey -fill black -stroke black -strokewidth 10.8 -draw "line 0,124 1648,124" 030.png
convert -density 300 -depth 8 -size 1648x124 canvas:lightgrey -fill black -stroke black -strokewidth 10.8 -draw "line 0,124 1648,124" 040.png
convert -density 300 -depth 8 -size 1648x124 canvas:lightgrey -fill black -stroke black -strokewidth 10.8 -draw "line 0,124 1648,124" 050.png
convert -density 300 -depth 8 -size 1648x124 canvas:lightgrey -fill black -stroke black -strokewidth 10.8 -draw "line 0,124 1648,124" 060.png
convert -density 300 -depth 8 -size 1648x124 canvas:lightgrey -fill black -stroke black -strokewidth 10.8 -draw "line 0,124 1648,124" 070.png
convert -density 300 -depth 8 -size 1648x124 canvas:lightgrey -fill black -stroke black -strokewidth 10.8 -draw "line 0,124 1648,124" 080.png
convert -density 300 -depth 8 -size 1648x124 canvas:lightgrey -fill black -stroke black -strokewidth 10.8 -draw "line 0,124 1648,124" 090.png
